{
  "name": "vue-trend-component",
  "version": "0.1.0",
  "description": "Simple, elegant spark lines for Vue.js",
  "main": "dist/vue-trend-component.common.js",
  "module": "dist/vue-trend-component.esm.js",
  "unpkg": "dist/vue-trend-component.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "eslint src && jest",
    "dev": "vue build docs/app.js -c docs/vue.config.js",
    "docs:build": "vue build docs/app.js -c docs/vue.config.js --prod --dist ./docs/dist",
    "deploy": "npm run docs:build && gh-pages -d docs/dist",
    "build": "rollup -c --environment BUILD && uglifyjs dist/vue-trend-component.js > dist/vue-trend-component.min.js"
  },
  "keywords": [
    "vue",
    "trend",
    "svg",
    "graph",
    "data-visualization",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/QingWei-Li/vue-trend-component.git"
  },
  "author": "qingwei-li <cinwell.li@gmail.com> (https://github.com/QingWei-Li)",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.0.1",
    "gh-pages": "^0.12.0",
    "hanabi": "^0.4.0",
    "jest": "^19.0.2",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "uglify-js": "^2.8.10",
    "vue": "^2.2.0",
    "vue-github-badge": "^1.0.0",
    "vue-template-compiler": "^2.2.0"
  }
}
